#include <stdio.h>

int main()
{
    for(int x=0;x<10;x++)
    {
        if(x%2 !=0)
        {
            for(int y=0; y<x-1; y+=2)
            {
                printf(" ");
            }
            for(int y=x; y<10;y++)
            {
                for(int z=0;z<1;z++)
                {
                    printf("*");
                }
            }
            printf("\n");
        }
    }
    return 0;
}



